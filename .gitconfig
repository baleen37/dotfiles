[user]
  name = baleen37
  email = baleen37@gmail.com
[core]
  editor = vim
  excludesfile = ~/.gitignore
[i18n]
  commitEncoding = utf8
[alias]
  r = log --graph --abbrev-commit --decorate --format=format:'%C(bold blue)%h%C(reset) - %C(bold cyan)%aD%C(reset) %C(bold green)(%ar)%C(reset)%C(bold yellow)%d%C(reset)%n''          %C(white)%s%C(reset) %C(dim white)- %an%C(reset)' --all
  st = status
  co = checkout
  ci = commit
  b = branch
[mergetool "vimdiff"]
  keepBackup = false
